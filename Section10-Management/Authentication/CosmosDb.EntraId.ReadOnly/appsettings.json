    // Authenticate using the Entra ID tenant ID, and the Read-Only application's client ID and client secret
    "TenantId": "[TENANT-ID]",
    "ClientId": "[CLIENT-ID]",
    "ClientSecret": "[CLIENT-SECRET]"
}
